{
  "name": "theory-engine",
  "version": "1.0.0",
  "description": "A comprehensive music theory library for JavaScript/TypeScript applications with chord analysis, scale generation, Circle of Fifths visualization, and guitar chord diagrams",
  "main": "./dist/lib/theory-core/index.js",
  "types": "./dist/lib/theory-core/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/lib/theory-core/index.d.ts",
      "import": "./dist/lib/theory-core/index.js",
      "require": "./dist/lib/theory-core/index.js"
    },
    "./guitar-chords": {
      "types": "./dist/lib/theory-core/guitar-chords/index.d.ts",
      "import": "./dist/lib/theory-core/guitar-chords/index.js",
      "require": "./dist/lib/theory-core/guitar-chords/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:lib": "tsc -p tsconfig.lib.json",
    "clean": "rm -rf dist",
    "prepare": "npm run build:lib",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "npm run clean && npm run build:lib && npm test"
  },
  "keywords": [
    "music-theory",
    "chord-analysis",
    "scale-generation",
    "guitar-chords",
    "circle-of-fifths",
    "harmony",
    "composition",
    "music-education",
    "tonal",
    "typescript",
    "music",
    "chords",
    "scales",
    "intervals",
    "voicings",
    "fretboard",
    "diagram",
    "progression",
    "cadence"
  ],
  "author": "Theory Engine Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mkoetter/theory_engine.git"
  },
  "bugs": {
    "url": "https://github.com/mkoetter/theory_engine/issues"
  },
  "homepage": "https://github.com/mkoetter/theory_engine#readme",
  "peerDependencies": {
    "tonal": "^6.4.0"
  },
  "dependencies": {
    "tonal": "^6.4.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@types/node": "^24.9.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "next": "^15.5.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
